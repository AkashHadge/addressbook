pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
           git "https://github.com/AkashHadge/addressbook"
                sh 'mvn compile'
            }
            
           }
           
       stage('Code-Review') {
            steps {
                sh 'pmd:pmd'
            }
            
           }     
           
      stage('test') {
            steps {
                sh 'test'
            }
            
           }          
       stage('Check') {
            steps {
                sh 'cobertura:cobertura'
            }
            
           }               
      stage('Package') {
            steps {
                sh 'package'
            }
            
           }          
           
                   
        }
      } 
